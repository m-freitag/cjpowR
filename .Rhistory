library(cjpowR)
n_amce <- function(alpha = 0.05, power = 0.8, levels = 2,
delta0 = 0.5, delta1){
(levels/2)/(delta1^2) * (qnorm(p = 1- alpha/2) + qnorm(p = power))^2*
(
((delta0 + delta1)*(1 - (delta0 + delta1)))/0.5 +
((delta0)*(1 - delta0))/0.5
)
